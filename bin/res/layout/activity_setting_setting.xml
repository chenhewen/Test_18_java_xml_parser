<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:background="@color/menu_setting_title" >
    </FrameLayout>

    <include
        android:id="@+id/title_setting_setting"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        layout="@layout/common_title_layout" />

    <com.gto.zero.zboost.common.ui.floatlistview.FloatingGroupExpandableListView
        android:id="@+id/container_setting_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/title_setting_setting"
        android:layout_marginTop="0dp"
        android:background="#FFFFFF"
        android:cacheColorHint="#00000000"
        android:divider="#00000000"
        android:dividerHeight="0dp"
        android:drawSelectorOnTop="false"
        android:listSelector="#00000000"
        android:overScrollFooter="@null"
        android:overScrollMode="never"
        android:scrollbars="none" />

</RelativeLayout>